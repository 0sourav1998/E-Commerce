{
    "version": 2,
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "src": "/(.*\\.js)",
        "dest": "/$1"
      },
      {
        "src": "/(.*\\.css)",
        "dest": "/$1"
      },
      {
        "src": "/(.*\\.json)",
        "dest": "/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  